<launch>
	
	<param name="/use_sim_time" value="true"/>
	
	<node name="traj_process" pkg="velo_scanmatch" type="traj_process" output="screen" args="-traj_filename /code/jupyter_notebook/data/bags/bus2calib/bus2-interf-left_forward20.pbstream -bag_filenames /code/jupyter_notebook/data/bags/bus2calib/bus2-interf-left_forward.bag">
		<!--<param name="pc_topic" value="/velodyne_top/velodyne_points"/>-->
		<param name="pc_topic" value="/velodyne_front/velodyne_points"/>
		<param name="publish_pc" value="false"/>
		<param name="use_ref" value="false"/>
		<param name="calibrate" value="true"/>

		<param name="mode" value="6"/>
		<param name="search_resolution" value="0.1"/>
		<param name="half_width_search" value="5"/>

		<param name="search_resolution_xyz" value="0.01"/>
		<param name="half_width_search_xyz" value="0.50"/>

		<param name="search_resolution_rpy" value="0.1"/>
		<param name="half_width_search_rpy" value="5"/>

		<param name="init_x" value="0.0"/>
		<param name="init_y" value="0.0"/>
		<param name="init_z" value="0.0"/>

		<param name="init_roll" value="0.0"/>
		<param name="init_pitch" value="0.0"/>
		<param name="init_yaw" value="0.0"/>

		<param name="starting_mssg_number" value="0"/>
		<param name="ending_mssg_number" value="15000.0"/>
		
	</node>
	<node if="false" pkg="rviz" type="rviz" name="rviz" args="-d $(find velo_scanmatch)/rviz_cfg/calib_visualize.rviz" required="true"/>
</launch>
